{
  "build": {
    "beforeDevCommand": "pnpm dev",
    "beforeBuildCommand": "pnpm build",
    "devPath": "http://localhost:1420",
    "distDir": "../dist",
    "withGlobalTauri": false
  },
  "package": {
    "productName": "oneboot-sdk-client",
    "version": "0.0.1"
  },
  "tauri": {
    "bundle": {
      "windows": {
        "allowDowngrades": true,
        "certificateThumbprint": null,
        "digestAlgorithm": null,
        "nsis": null,
        "timestampUrl": null,
        "tsp": false,
        "webviewFixedRuntimePath": null,
        "webviewInstallMode": {
          "silent": true,
          "type": "embedBootstrapper"
        },
        "wix": null
      },
      "active": true,
      "targets": ["msi", "app", "updater"],
      "identifier": "com.github.alphafoxz.oneboot.sdk.client",
      "icon": ["icons/32x32.png", "icons/128x128.png", "icons/128x128@2x.png", "icons/icon.icns", "icons/icon.ico"],
      "category": "DeveloperTool"
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "oneboot-sdk-client",
        "minWidth": 800,
        "minHeight": 600,
        "width": 1366,
        "height": 768,
        "transparent": false,
        "fileDropEnabled": false
      }
    ]
  },
  "plugins": {
    "shell": {
      "open": true
    },
    "updater": {
      "active": false,
      "endpoints": ["https://releases.myapp.com/{{target}}/{{arch}}/{{current_version}}"],
      "dialog": true,
      "pubkey": "dW50cnVzdGVkIGNvbW1lbnQ6IG1pbmlzaWduIHB1YmxpYyBrZXk6IDIyMDc1NjZBNDk5NkM0NkMKUldSc3hKWkphbFlISW1KZkJGRDBja1FoZ28xTlcwM093R2hYUEV3bC9RSzJPd2VsMTZuUmtzSE4K"
    }
  }
}
